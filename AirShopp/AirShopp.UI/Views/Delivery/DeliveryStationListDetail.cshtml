@model AirShopp.Domain.DeliveryStation
@{
    Layout = "~/Views/Shared/_backEndLayout.cshtml";
}
@Styles.Render("~/bundles/AirShopp/Inventory/Css")
@Scripts.Render("~/bundles/AirShopp/Inventory/js")
<!-- 商城快讯 begin -->
<style>
    .member-cancel span {
        text-align: center;
        float: left;
        height: 35px;
        line-height: 35px;
        color: #a3a3a3;
    }

    .be11 {
        width: 380px;
    }

    .be22 {
        width: 180px;
    }

    .be33 {
        width: 185px;
    }

    .be44 {
        width: 85px;
    }

    .be55 {
        width: 53px;
    }

    .member-circle div {
        text-align: center;
        float: left;
        height: 35px;
        line-height: 35px;
        border-right: 1px solid #e0e0e0;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
    }

    .ci11 {
        width: 380px;
    }

    .ci22 {
        width: 179px;
    }

    .ci33 {
        width: 184px;
    }

    .ci44 {
        width: 84px;
    }

    .ci55 {
        width: 52px;
    }

    .member-about select {
        width: 100px;
        height: 26px;
        line-height: 26px;
        border-radius: 3px;
        margin-right: 10px;
        border: 1px solid #e0e0e0;
    }
</style>
<div class="member-right fr">
    @if (Model.StationLevel == 3)
    {

        <div class="member-head">
            <div class="member-heels fl"><h2>派送员</h2></div>
        </div>
        <div class="member-border">
            <div class="member-return H-over">
                <div class="member-cancel clearfix">
                    <span class="be22">区域</span>
                    <span class="be33">名称</span>
                    <span class="be11">地址</span>
                    <span class="be44">经度</span>
                    <span class="be44">纬度</span>
                    <span class="be55">等级</span>
                </div>
                <div class="member-sheet clearfix">
                    <ul>
                        @foreach(var item in Model.Couriers) {
                            <li>
                                <div class="member-circle clearfix">
                                    <div class="ci22">1</div>
                                    <div class="ci33">2</div>
                                    <div class="ci11">3</div>
                                    <div class="ci44">4</div>
                                    <div class="ci44">5</div>
                                    <div class="ci55" style="border:none;">6</div>
                                </div>
                            </li>
                        }
                    </ul>
                </div>
            </div>
            <div class="clearfix" style="padding:30px 20px;">
                <div class="fr pc-search-g pc-search-gs">
                </div>
            </div>
        </div>
    }
    else
    {
        <div class="member-head">
            <div class="member-heels fl"><h2>子级派送点</h2></div>
        </div>
        <div class="member-border">
            <div class="member-return H-over">
                <div class="member-cancel clearfix">
                    <span class="be22">区域</span>
                    <span class="be33">名称</span>
                    <span class="be11">地址</span>
                    <span class="be44">经度</span>
                    <span class="be44">纬度</span>
                    <span class="be55">等级</span>
                </div>
                <div class="member-sheet clearfix">
                    <ul>
                        @foreach (var item in Model.DeliveryStations)
                        {
                            <li>
                                <div class="member-circle clearfix">
                                    <div class="ci22">XXX</div>
                                    <div class="ci33">@Html.ActionLink(@item.Name, "DeliveryStationListDetail", new { deliveryOrderId = @item.Id })</div>
                                    <div class="ci11">@item.Address</div>
                                    <div class="ci44">@item.Latitude</div>
                                    <div class="ci44">@item.Longitude</div>
                                    <div class="ci55" style="border:none;">@item.StationLevel</div>
                                </div>
                            </li>
                        }
                    </ul>
                </div>
            </div>
            <div class="clearfix" style="padding:30px 20px;">
                <div class="fr pc-search-g pc-search-gs">
                </div>
            </div>
        </div>
    }
</div>