                                <div class="cci4 amanda1"><div><p>@Model.AllStatusViewModel.AllOrder[i].OrderDate.ToShortDateString()</p></div></div>
                                
                                @if (Model.AllStatusViewModel.AllOrder[i].Returns.Count() == 0)
